{
  "name": "@chris-lally/ui-branding",
  "version": "0.0.0",
  "private": true,
  "description": "Branding-focused UI components for the lally ecosystem.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisLally/lally-ui"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./registry": "./dist/registry.js"
  },
  "scripts": {
    "build": "pnpm exec tsup --config tsup.config.ts",
    "lint": "eslint . --max-warnings 0",
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit",
    "verify:pack": "pnpm run build && npm pack --dry-run",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "^9.39.1",
    "tsup": "^8.5.0",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
