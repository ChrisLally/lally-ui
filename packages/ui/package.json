{
  "name": "@chris-lally/ui",
  "version": "0.1.0-alpha.5",
  "description": "CLI and distribution entrypoint for @chris-lally/ui workflows",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisLally/lally-ui"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "lally-ui": "./dist/bin/lally-ui.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "pnpm --filter @chris-lally/ui-branding run build && pnpm exec tsup --config tsup.config.ts",
    "check-types": "pnpm --filter @chris-lally/ui-branding run build && tsc --noEmit",
    "dev": "ts-node src/bin/lally-ui.ts",
    "verify:pack": "pnpm run build && npm pack --dry-run"
  },
  "dependencies": {
    "@chris-lally/ui-branding": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "5.9.2"
  }
}
